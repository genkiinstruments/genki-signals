{
	"name": "dashboard",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"tsc": "tsc",
		"vite:dev": "vite dev --open",
		"vite:build": "vite build --watch",
		"copy:wasm": "cpy node_modules/scichart/_wasm/scichart2d.wasm ./  --flat",
		"copy:data": "cpy node_modules/scichart/_wasm/scichart2d.data ./  --flat",
		"dev": "concurrently npm:copy:wasm npm:copy:data npm:vite:dev",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@rgossiaux/svelte-headlessui": "^1.0.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.13.0",
		"@tsconfig/svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"@vitejs/plugin-vue": "^4.1.0",
		"concurrently": "^7.6.0",
		"cpy-cli": "^4.2.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.6",
		"prettier-plugin-svelte": "^2.9.0",
		"socket.io-client": "^4.6.1",
		"svelte": "^3.57.0",
		"svelte-check": "^3.1.4",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.5.0",
		"typescript": "^5.0.2",
		"vite": "^4.2.1",
		"vitest": "^0.29.7"
	},
	"type": "module",
	"dependencies": {
		"scichart": "^3.0.317"
	}
}
